{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: OTI MNIST",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--dataset", "MNIST",
                "--model", "LeNet",
                "--selection", "OTI",
                "--num_exp", "1",
                "--epochs", "5",
                "--selection_epochs", "5",
                "--data_path", "./data",
                "--gpu", "0", "1", "2",
                "--optimizer", "SGD",
                "--lr", "0.1",
                "--scheduler", "CosineAnnealingLR",
                "--save_path", "./results",
                "--num_gpus", "3",
                "--oti_mode", "scores",
                "--fraction", "0.1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/.venv/bin/python"
        }
    ]
}